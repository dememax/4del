{
    "C_Cpp.default.compileCommands": "${workspaceFolder}/builddir/compile_commands.json",
    "C_Cpp.default.configurationProvider": "mesonbuild.mesonbuild",
    "mesonbuild.defaultOptions": [
        "buildtype=debug",
        "build-tools-source=system",
        "base=enabled",
        "good=enabled",
        "bad=disabled",
        "ugly=disabled",
        "libav=disabled",
        "devtools=disabled",
        "ges=disabled",
        "rtsp_server=disabled",
        "python=disabled",
        "examples=disabled",
        "doc=disabled",
        "tests=disabled",
        "nls=disabled",
        "libnice=disabled",
        "gst-plugins-base:gl=disabled",
        "gst-plugins-base:theora=disabled",
        "gst-plugins-base:pango=disabled",
        "gst-plugins-good:audioparsers=disabled",
        "gst-plugins-good:isomp4=disabled",
        "gst-plugins-good:matroska=disabled",
        "gst-plugins-good:flac=disabled",
        "gst-plugins-good:soup=disabled",
        "gst-plugins-good:pulse=disabled",
        "gst-plugins-good:avi=disabled",
        "gst-plugins-good:vpx=disabled",
        "gst-plugins-good:lame=disabled",
        "gst-plugins-good:gtk=disabled",
        "gst-plugins-good:cairo=disabled",
        "gst-plugins-good:rtp=disabled",
        "gst-plugins-good:rtsp=disabled",
        "gst-plugins-good:rtpmanager=disabled",
        "gst-plugins-good:flv=disabled",
        "gst-plugins-good:jack=disabled",
    ]
    "cSpell.words": [
        "BHMD",
        "DEMENTYEV",
        "libwebp",
        "Maxime",
        "sagui",
        "SAGUI",
    ]
    "editor.rulers": [80],
    "editor.renderWhitespace": "all",
    "editor.wordWrap": "off",
    "editor.fontFamily": "Ubuntu Sans, Arial, sans-serif",
    "editor.fontSize": "14",
    "terminal.integrated.fontFamily": "Ubuntu Mono, Courier New, monospace",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.lineHeight": 1.2
}
